// This is your Prisma schema file,
// learn more about it in the docs: https://pris.ly/d/prisma-schema

generator client {
  provider = "prisma-client-js"
}

datasource db1 {
  provider = "postgresql"
  url      = env("DB_URL")
}

model User {
  id    Int     @default(autoincrement()) @id
  email String  @unique
  name  String?
}
